<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="476.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ReportGenerationController">
   <children>
      <Label fx:id="ReportWindowLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="15.0" layoutY="14.0" prefHeight="59.0" prefWidth="447.0" text="Reports Window" underline="true">
         <font>
            <Font name="Bookman Old Style Bold" size="45.0" />
         </font>
      </Label>
      <ComboBox fx:id="ChooseReportFromListBox" layoutX="79.0" layoutY="123.0" onAction="#ChooseReport" prefHeight="26.0" prefWidth="203.0" promptText="Choose Report From List" />
      <Label fx:id="EnterRequstedReportLabel" layoutX="79.0" layoutY="172.0" text="Enter The Requsted Report:" />
      <TextField fx:id="RequestReportTextField" layoutX="77.0" layoutY="213.0" onAction="#EnterRequstedReport" prefHeight="26.0" prefWidth="203.0" />
      <Button fx:id="DisplayReportButton" layoutX="86.0" layoutY="267.0" mnemonicParsing="false" onAction="#DisplayReport" text="Display Report" />
      <Button fx:id="BackButton" layoutX="89.0" layoutY="319.0" mnemonicParsing="false" onAction="#BackToMenu" text="Back To Menu" />
   </children>
</AnchorPane>
