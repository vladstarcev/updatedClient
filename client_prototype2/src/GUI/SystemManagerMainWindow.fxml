<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SystemManagerMainWindowController">
   <children>
      <Label layoutX="66.0" layoutY="29.0" text="System Manager Menu" underline="true">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="addCourseBtn" layoutX="148.0" layoutY="114.0" mnemonicParsing="false" onAction="#addCourse" prefHeight="53.0" prefWidth="166.0" text="Add new course" />
      <Button fx:id="deleteCourseBtn" layoutX="143.0" layoutY="191.0" mnemonicParsing="false" onAction="#deleteCourse" prefHeight="53.0" prefWidth="174.0" text="Delete existing course" />
      <Button fx:id="LogOutButton" layoutX="202.0" layoutY="289.0" mnemonicParsing="false" onAction="#LogOutSystemManager" text="Log Out" />
   </children>
</AnchorPane>
