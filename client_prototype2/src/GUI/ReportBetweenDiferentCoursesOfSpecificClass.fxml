<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="863.0" prefWidth="1140.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ReportBetweenCoursesController">
   <children>
      <Label fx:id="ReportLabel1" layoutX="151.0" layoutY="8.0" text="Report Between Differnt Courses" underline="true">
         <font>
            <Font name="Bookman Old Style Bold" size="48.0" />
         </font>
      </Label>
      <Label fx:id="ReportLabel2" layoutX="324.0" layoutY="65.0" text="Of Specific Class" underline="true">
         <font>
            <Font name="Bookman Old Style Bold" size="48.0" />
         </font>
      </Label>
      <BarChart fx:id="ReportChart" layoutX="41.0" layoutY="331.0" prefHeight="506.0" prefWidth="734.0">
        <xAxis>
          <CategoryAxis fx:id="SpecificClassAxis" label="Spesific Class" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="DifferentCoursesAxis" label="Different Courses In Class" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="BackButton" layoutX="903.0" layoutY="737.0" mnemonicParsing="false" onAction="#BackToMenu" prefHeight="41.0" prefWidth="143.0" text="Back To Menu" />
      <ComboBox fx:id="ChooseComboBox" layoutX="41.0" layoutY="156.0" onAction="#ChooseClass" prefHeight="41.0" prefWidth="168.0" promptText="Choose:" />
      <Label fx:id="ChooseLabel" layoutX="41.0" layoutY="126.0" text="Choose Class ID From The List Below:" />
      <TextField fx:id="SemesterTextField" layoutX="41.0" layoutY="246.0" prefHeight="41.0" prefWidth="475.0" />
      <Label fx:id="SemesterLabel" layoutX="41.0" layoutY="214.0" text="Enter 4 Semesters ID Seperated By ( ):" />
      <Button fx:id="CheckSemesterButton" layoutX="581.0" layoutY="246.0" mnemonicParsing="false" onAction="#CheckSemesterID" prefHeight="41.0" prefWidth="198.0" text="Check Semester ID" />
      <Button fx:id="DisplayButton" layoutX="903.0" layoutY="660.0" mnemonicParsing="false" onAction="#DisplayChart" prefHeight="41.0" prefWidth="143.0" text="Display Chart" />
   </children>
</AnchorPane>
