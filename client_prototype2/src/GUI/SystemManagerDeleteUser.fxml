<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controllers.DeleteUserController">
   <children>
      <Label fx:id="enterUserLbl" layoutX="192.0" layoutY="66.0" text="Enter user ID you want to delete:" />
      <TextField fx:id="idTxt" layoutX="207.0" layoutY="112.0" />
      <Button fx:id="deleteBtn" layoutX="253.0" layoutY="193.0" mnemonicParsing="false" onAction="#deleteUser" text="Delete user" />
      <Button fx:id="cancelBtn" layoutX="75.0" layoutY="319.0" mnemonicParsing="false" onAction="#backToMenu" text="Cancel" />
   </children>
</AnchorPane>
