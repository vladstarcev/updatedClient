<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="571.0" prefWidth="786.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.OpenNewSemesterController">
   <children>
      <Label fx:id="OpenSemesterLable" layoutX="40.0" layoutY="14.0" text="Open New Semester Window" underline="true">
         <font>
            <Font name="Bookman Old Style Bold" size="48.0" />
         </font>
      </Label>
      <Label fx:id="FillFormLable" layoutX="40.0" layoutY="95.0" text="Fill The Form For Opening New Semester:" />
      <Label fx:id="SemesterIDLable" layoutX="49.0" layoutY="132.0" text="Enter Semester Id For New Semester:" />
      <TextField fx:id="SemesterIdTextField" layoutX="49.0" layoutY="163.0" onAction="#EnterSemesterID" prefHeight="33.0" prefWidth="418.0" />
      <Label fx:id="TypeLable" layoutX="49.0" layoutY="214.0" text="Choose Type For New Semester:" />
      <Label fx:id="StatusLable" layoutX="49.0" layoutY="303.0" text="Choose Status For New Semester:" />
      <Label fx:id="AcademicAcIDLable" layoutX="49.0" layoutY="402.0" text="Enter Academic Activities ID Seperated By Coma ( ):" />
      <TextField fx:id="AcademicAcTextField" layoutX="49.0" layoutY="433.0" onAction="#EnterAcademicAcID" prefHeight="33.0" prefWidth="418.0" />
      <Button fx:id="SendButton" layoutX="49.0" layoutY="505.0" mnemonicParsing="false" onAction="#SendSemesterForm" prefHeight="33.0" prefWidth="144.0" text="Open Semester" />
      <Button fx:id="BackButton" layoutX="535.0" layoutY="512.0" mnemonicParsing="false" onAction="#BackToMenu" prefHeight="33.0" prefWidth="178.0" text="Back To Menu" />
      <MenuButton fx:id="SemesterMenu" layoutX="49.0" layoutY="246.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="171.0" text="Semester Type">
        <items>
          <MenuItem fx:id="SemesterA" mnemonicParsing="false" onAction="#SemA" text="A" />
          <MenuItem fx:id="SemesterB" mnemonicParsing="false" onAction="#SemB" text="B" />
        </items>
      </MenuButton>
      <MenuButton fx:id="StatusMenu" layoutX="49.0" layoutY="339.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="171.0" text="Status">
        <items>
          <MenuItem fx:id="CurrentSEMMenu" mnemonicParsing="false" onAction="#CurrentSEM" text="Current" />
        </items>
      </MenuButton>
      <Button fx:id="CheckIDButton" layoutX="535.0" layoutY="162.0" mnemonicParsing="false" onAction="#CheckSemesterID" prefHeight="33.0" prefWidth="144.0" text="Check Semester ID" />
      <Button fx:id="CheckActivitiesButton" layoutX="542.0" layoutY="432.0" mnemonicParsing="false" onAction="#CheckAcademicActivities" prefHeight="33.0" prefWidth="144.0" text="Check Activities" />
   </children>
</AnchorPane>
